------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

SIDEBAR COMPONENT:

import React, { useState } from 'react';
import './App.css';

const Sidebar = () => {
  const [isOpen, setIsOpen] = useState(true);

  const toggleSidebar = () => {
    setIsOpen(!isOpen);
  };

  return (
    <div className="flex">
      {/* Sidebar */}
      <div className={`transition-transform duration-300 ease-in-out ${isOpen ? 'w-64' : 'w-20'} h-screen bg-gray-800 text-white`}>
        <div className="flex justify-between items-center p-4">
          <h2 className={`text-2xl font-semibold ${isOpen ? 'block' : 'hidden'}`}>My Sidebar</h2>
          <button onClick={toggleSidebar} className="text-white focus:outline-none">
            {isOpen ? 'Close' : 'Open'}
          </button>
        </div>
        <ul className="mt-8">
          <li className="p-4 hover:bg-gray-700 cursor-pointer">Dashboard</li>
          <li className="p-4 hover:bg-gray-700 cursor-pointer">Settings</li>
          <li className="p-4 hover:bg-gray-700 cursor-pointer">Profile</li>
          <li className="p-4 hover:bg-gray-700 cursor-pointer">Messages</li>
          <li className="p-4 hover:bg-gray-700 cursor-pointer">Log out</li>
        </ul>
      </div>

      {/* Content */}
      <div className="flex-1 p-8">
        <h1 className="text-3xl font-bold">Main Content Area</h1>
        <p className="mt-4 text-lg">Here goes the main content of the page!</p>
      </div>
    </div>
  );
};

function App() {
  return (
    <div className="App">
      <Sidebar />
    </div>
  );
}

export default App;
